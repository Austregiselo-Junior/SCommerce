    <SolidColorBrush x:Key="AudioSampleButtonBorderBrush_Hover" Color="Transparent" />
    <SolidColorBrush x:Key="AudioSampleTextForeground_Hover" Color="#FAFAFA" />
    <SolidColorBrush x:Key="CommonBorderColor" Color="Transparent" />
    <SolidColorBrush x:Key="AudioSampleButtonBorderBrush_Pressed" Color="Transparent" />
    <SolidColorBrush x:Key="AudioSampleTextForeground_Pressed" Color="#FAFAFA" />
</ResourceDictionary>
